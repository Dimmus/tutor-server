<%= link_to 'Create user', new_admin_user_path %>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= link_to 'edit', edit_admin_user_path(user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
